<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.intelliinvest.data.model">

    <class name="MagicNumberData">
    	<id name="symbol" column="symbol">
    		<generator class="assigned"/>
    	</id>
    	<property name="magicNumber" column="magicNumber"/>
    	<property name="movingAverage" column="movingAverage"/>
    	<property name="pnl" column="pnl"/>
    </class>
    
</hibernate-mapping>